<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN" "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">

<urlrewrite>
    <rule>
        <from>/api/(.*)</from>
        <to>/api/$1</to>
    </rule>
    <rule>
        <from>/images/(.*)</from>
        <to>/images/$1</to>
    </rule>
    <rule>
        <from>/stylesheets/(.*)</from>
        <to>/stylesheets/$1</to>
    </rule>
    <rule>
        <from>/javascripts/(.*)</from>
        <to>/javascripts/$1</to>
    </rule>
    <rule>
        <from>/(.*)</from>
        <to>/pages/$1</to>
    </rule>
    <outbound-rule>
        <from>/pages/(.*)</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>